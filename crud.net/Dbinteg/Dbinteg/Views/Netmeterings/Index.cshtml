@model IEnumerable<Dbinteg.Netmetering>

@{
    ViewBag.Title = "Solat Plant And NetMetering";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Month)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MonthlyConsumption)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NetConsumption)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MonthlyBill)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BillAfterPlan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Savings)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.payback)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Month)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MonthlyConsumption)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NetConsumption)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MonthlyBill)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BillAfterPlan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Savings)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.payback)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>


